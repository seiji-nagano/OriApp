    <%= form_with(model: tweet, local: true) do |form| %>
      <%= form.text_field :title, placeholder: "タイトル" %>
      <%= form.file_field :video %>
      <h3>練習カテゴリー</h3>
      <%= form.collection_select(:category_id, Category.all, :id, :category, {}) %>
      <%= form.text_area :text, placeholder: "内容", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>